<!-- admin.ejs -->


<body>
    <h1><%= title %></h1>
    <table>
        <thead>
            <tr>
                <th>First Name</th>
                <th>Last Name</th>
                <th>Email</th>
                <!-- Add more fields as needed -->
            </tr>
        </thead>
        <tbody>
            <% users.forEach(user => { %>
                <tr>
                    <td><%= user.firstName %></td>
                    <td><%= user.lastName %></td>
                    <td><%= user.email %></td>
                    <!-- Add more fields as needed -->
                </tr>
            <% }); %>
        </tbody>
    </table>
</body>





